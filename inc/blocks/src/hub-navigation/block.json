{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "greenergy/hub-navigation",
    "title": "نظام التنقل (Hub)",
    "category": "greenergy-blocks",
    "icon": "randomize",
    "description": "نوافذ التنقل بين التدريبات والمقالات التعليمية",
    "editorScript": "greenergy-blocks-editor",
    "attributes": {
        "coursesLabel": {
            "type": "string",
            "default": "التدريبات"
        },
        "coursesUrl": {
            "type": "string",
            "default": ""
        },
        "articlesLabel": {
            "type": "string",
            "default": "المقالات التعليمية"
        },
        "articlesUrl": {
            "type": "string",
            "default": ""
        },
        "activeTab": {
            "type": "string",
            "default": "courses"
        }
    },
    "render": "file:./render.php"
}