{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "greenergy/all-courses",
    "title": "All Courses",
    "category": "greenergy-blocks",
    "icon": "grid-view",
    "editorScript": "greenergy-blocks-editor",
    "attributes": {
        "postsPerPage": {
            "type": "number",
            "default": 12
        },
        "featuredSelectedPosts": {
            "type": "array",
            "default": []
        },
        "featuredCount": {
            "type": "number",
            "default": 4
        },
        "selectedCategories": {
            "type": "array",
            "default": []
        }
    },
    "render": "file:./render.php"
}